{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_Source_dataflows",
						"type": "DatasetReference"
					},
					"name": "source1Table"
				},
				{
					"dataset": {
						"referenceName": "ds_source_DataFlows_sqlTable2",
						"type": "DatasetReference"
					},
					"name": "source2Table2",
					"description": "Employessinfo Table"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          name as string,",
				"          dep as string,",
				"          salary as decimal(10,2),",
				"          last_updated as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1Table",
				"source(output(",
				"          id as integer,",
				"          location as string,",
				"          mobilenum as string,",
				"          address as string,",
				"          last_updated as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2Table2",
				"source1Table, source2Table2 join(source1Table@id == source2Table2@id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}